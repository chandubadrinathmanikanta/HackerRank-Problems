#include <stdio.h>

int main()
{
	int var1, var2, add, sub, mul, div;
	//Input values of var1 and var2
	//Calculate values of add, sub, mul and div
    scanf("%d %d", &var1, &var2);
    add = var1 + var2;
    sub = var1 - var2;
    mul = var1 * var2;
	div = var1/var2;



	printf("%d %d %d %d", add, sub, mul, div);
	return 0;
}
