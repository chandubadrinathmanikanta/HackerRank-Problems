#include <stdio.h>

int main() {
    int N;
    
    // Read the input value N
    scanf("%d", &N);
    
    // Loop through each row
    for (int i = 1; i <= N; i++) {
        // Loop through each number in the row
        for (int j = 1; j <= i; j++) {
            printf("%d", j);  // Print the number
        }
        // After each row, print a new line
        printf("\n");
    }

    return 0;
}
