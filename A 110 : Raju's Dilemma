#include<stdio.h>

int main() {
    int N, X;
    int count = 0;
    
    scanf("%d %d", &N, &X);
    
    int arr[N];
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    
    for(int i = 0; i < N; i++) {
        if(arr[i] % X == 0) {
            count++;
        }
    }
    
    printf("%d", count);
    
    return 0;
}
