#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main()
{
	char ch;
	cin>>ch;
	switch(ch)
	{
		case 'a' : cout<<"vowel"; break;
		case 'e' : cout<<"vowel"; break;
        case 'i' : cout<<"vowel"; break;
        case 'o' : cout<<"vowel"; break;
        case 'u' : cout<<"vowel"; break;
            
		//write the cases for i, o, and u.
	

		default : cout<<"not vowel"; break;
	}	
	return 0;
}
