#include<stdio.h>
#include<string.h>

int main()
{
    char str[50];
    
    // input string here using scanf
    scanf("%s", str);  // No ampersand is needed because 'str' is already a pointer to the first character
    
    // print string here using printf
    printf("%s\n", str);  // Prints the string with a newline at the end
    
    return 0;
}
