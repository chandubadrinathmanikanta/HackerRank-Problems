#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int fibonacci(int N){
    if(N == 0) return 0;
    if(N == 1) return 1;
    
    return fibonacci(N - 1) + fibonacci(N - 2);
}
int main() {
    int N;
    scanf("%d", &N);
    printf("%d\n", fibonacci(N));

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
