#include <stdio.h>
int main()
{
    int i, a[10]; // Declare an array of size 10
    for (i = 0; i < 10; i++) 
        scanf("%d", &a[i]);

    // Print elements whose indices are odd (i.e. arr[1], arr[3], arr[5], ...)
    for (i = 1; i < 10; i += 2) {
        printf("%d ", a[i]);
    }
    return 0;
}
